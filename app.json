{
  "expo": {
    "name": "Meetowner",
    "slug": "meetowner",
    "version": "4.0.5",
    "orientation": "portrait",
    "sdkVersion": "52.0.0",
    "icon": "./assets/logoicon.png",
    "packagerOpts": {
      "minify": true
    },
    "splash": {
      "image": "./assets/logoicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "description": "MeetOwner app for Realstate Property listings,brokers,construction,buildings,Apartments,Flats etc",
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "icon": "./assets/logoicon.png",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBmei9lRUUfJI-kLIPNBoc2SxEkwhKHyvU"
      },
      "buildNumber": "1.7",
      "minimumOsVersion": "13.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "MeetOwner needs access to your location to display nearby properties based on your current location, improving property search results.",
        "NSLocationAlwaysUsageDescription": "MeetOwner needs access to your location in the background to provide continuous property suggestions and improve location-based services.",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification",
          "fetch",
          "remote-notification"
        ],
        "NSCameraUsageDescription": "MeetOwner needs access to your camera to take photos for property listings and profile pictures, making it easier to showcase your properties.",
        "NSPhotoLibraryUsageDescription": "MeetOwner needs access to your photo library so you can upload images for property listings and profile pictures, enhancing your property visibility.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.meetowner.app"
    },
    "android": {
      "versionCode": 26,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBmei9lRUUfJI-kLIPNBoc2SxEkwhKHyvU"
        }
      },
      "enableProguardInReleaseBuilds": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "READ_CONTACTS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.meetowner.app",
      "notification": {
        "icon": "./assets/logoicon.png",
        "color": "#FF231F7C"
      }
    },
    "web": {
      "favicon": "./assets/Favicon@6x.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/Favicon@10x.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/logoicon.png",
          "dark": {
            "image": "./assets/logoicon.png",
            "backgroundColor": "#000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "scheme": "Meetowner",
    "platforms": ["ios", "android"],
    "extra": {
      "googleClientIdWeb": "284031030947-v2kgmakcko9hcgb60vg0u4elnj9ur75u.apps.googleusercontent.com",
      "googleClientIdAndroid": "284031030947-v2kgmakcko9hcgb60vg0u4elnj9ur75u.apps.googleusercontent.com",
      "googleClientIdIOS": "284031030947-bkc3ea1vn5d150muvsu85nmrliap046n.apps.googleusercontent.com",
      "eas": {
        "projectId": "871b5e55-6c02-430a-9cc9-6d6fbb44af89"
      }
    },
    "owner": "meetowner1"
  }
}
